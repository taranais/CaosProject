{
    "version": "0.0.1-alpha",
    "authors": ["Taranais"],
    "description": "creatures serie games tools",
    "buildOptions": {
            "emitEntryPoint": true
            },
    "configurations": {
        "Debug": {
        "emitEntryPoint": true,
        "buildOptions": {
            "define": ["DEBUG", "TRACE"],
            "emitEntryPoint": true
            }
        },
        "Release": {
        "emitEntryPoint": true,
        "buildOptions": {
            "define": ["RELEASE", "TRACE"],
            "emitEntryPoint": true,
            "optimize": true
            }
        }
    },
    "scripts": {
        "prebuild": "echo before building",
        "postbuild": "echo after building",
        "prepack": "echo before packing",
        "postpack": "echo after packing",
        "prerestore": "echo before restoring packages",
        "postrestore": "echo after restoring packages"
    },
    "dependencies": {
        "Microsoft.NETCore.App": {
            "type": "platform",
            "version": "1.0.0-rc2-3002702"
        },
        "System.IO": "4.1.0-*",
        "System.IO.MemoryMappedFiles": "4.0.0-*",
        "NLog": "4.4.0-*",
        "ImageProcessorCore": "1.0.0-alpha-860"
    },
    "frameworks": {
        "netcoreapp1.0": {
            "imports": "dnxcore50"
        }
    }
}
