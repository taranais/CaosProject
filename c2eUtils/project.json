{
    "version": "0.0.1-alpha",
    "authors": ["Taranais"],
    "description": "creatures serie games tools",
    "compilationOptions": {
             "emitEntryPoint": true
            },        
    "configurations": {
        "Debug": {
        "emitEntryPoint": true,
        "compilationOptions": {
            "define": ["DEBUG", "TRACE"],
             "emitEntryPoint": true
            }
        },
        "Release": {
        "emitEntryPoint": true,
        "compilationOptions": {
            "define": ["RELEASE", "TRACE"],
            "emitEntryPoint": true,
            "optimize": true
            }
        }
    },    
    "scripts": {
        "prebuild": "echo before building",
        "postbuild": "echo after building",
        "prepack": "echo before packing",
        "postpack": "echo after packing",
        "prerestore": "echo before restoring packages",
        "postrestore": "echo after restoring packages"
    },
    "dependencies": {
        "NETStandard.Library": "1.0.0-rc2-23811",
        "System.IO": "4.1.0-rc2-23811",
        "System.IO.MemoryMappedFiles": "4.0.0-rc2-23811",
        "NLog": "4.4.0-beta4",
        "ImageProcessorCore": "1.0.0-alpha-783"
    },
    "frameworks": {
        "dnxcore50": {}
    }
}